<% @ironic_services.each do |s| %>
command[check_<%= s %>]=/usr/lib/nagios/plugins/check_procs -w 2:10 -c 1:100 -a "<%= s %>"
<% end unless @ironic_services.nil? %>

<% @ironic_ports.each do |name,dest| %>
command[check_port_<%= name %>]=/usr/lib/nagios/plugins/check_tcp -H <%= @ironic_ip %> -p <%= dest %>
<% end unless @ironic_ports.nil? %>
