{
  "id": "template-magnum",
  "description": "Sets up OpenStack Magnum Containers as a Service",
  "attributes": {
    "magnum": {
      "debug": false,
      "verbose": true,
      "keystone_instance": "none",
      "database_instance": "none",
      "glance_instance": "none",
      "nova_instance": "none",
      "heat_instance": "none",
      "neutron_instance": "none",
      "rabbitmq_instance": "none",
      "service_user": "magnum",
      "service_password": "",
      "trustee": {
          "cluster_user_trust": false,
          "domain_name": "magnum",
          "domain_admin_name": "magnum_domain_admin",
          "domain_admin_password": ""
      },
      "api": {
        "protocol": "http",
        "bind_open_address": true,
        "bind_port": 9511
      },
      "db": {
        "password": "",
        "user": "magnum",
        "database": "magnum"
      },
      "cert": {
        "cert_manager_type": "x509keypair"
      },
      "ssl": {
        "certfile": "/etc/magnum/ssl/certs/signing_cert.pem",
        "keyfile": "/etc/magnum/ssl/private/signing_key.pem",
        "generate_certs": false,
        "insecure": false,
        "cert_required": false,
        "ca_certs": "/etc/magnum/ssl/certs/ca.pem"
      }
    }
  },
  "deployment": {
    "magnum": {
      "crowbar-revision": 1,
      "crowbar-applied": false,
      "schema-revision": 102,
      "element_states": {
        "magnum-server": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
        [ "magnum-server" ]
      ],
      "element_run_list_order": {
          "magnum-server": 110
      },
      "config": {
        "environment": "magnum-base-config",
        "mode": "full",
        "transitions": false,
        "transition_list": [
        ]
      }
    }
  }
}
