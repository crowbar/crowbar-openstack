{
  "id": "template-sahara",
  "description": "Sets up OpenStack Sahara Data Processing",
  "attributes": {
    "sahara": {
      "debug": false,
      "use_syslog": true,
      "keystone_instance": "none",
      "database_instance": "none",
      "glance_instance": "none",
      "nova_instance": "none",
      "heat_instance": "none",
      "neutron_instance": "none",
      "rabbitmq_instance": "none",
      "cinder_instance": "none",
      "service_user": "sahara",
      "service_password": "",
      "memcache_secret_key": "",
      "plugins": "vanilla,spark,storm,cdh,ambari,mapr",
      "api": {
        "protocol": "http",
        "bind_open_address": true,
        "bind_port": 8386
      },
      "db": {
        "password": "",
        "user": "sahara",
        "database": "sahara"
      },
      "use_barbican_key_manager": false
    }
  },
  "deployment": {
    "sahara": {
      "crowbar-revision": 0,
      "crowbar-applied": false,
      "schema-revision": 300,
      "element_states": {
        "sahara-server": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
        [ "sahara-server" ]
      ],
      "element_run_list_order": {
          "sahara-server": 110
      },
      "config": {
        "environment": "sahara-base-config",
        "mode": "full",
        "transitions": false,
        "transition_list": [
        ]
      }
    }
  }
}
