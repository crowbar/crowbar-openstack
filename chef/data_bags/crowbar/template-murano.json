{
  "id": "template-murano",
  "description": "Sets up OpenStack Murano Application Catalog",
  "attributes": {
    "murano": {
      "debug": false,
      "verbose": true,
      "use_syslog": true,
      "keystone_instance": "none",
      "database_instance": "none",
      "glance_instance": "none",
      "nova_instance": "none",
      "heat_instance": "none",
      "neutron_instance": "none",
      "rabbitmq_instance": "none",
      "service_user": "sahara",
      "service_password": "",
      "api": {
        "protocol": "http",
        "bind_open_address": true,
        "bind_port": 8082
      },
      "db": {
        "password": "",
        "user": "murano",
        "database": "murano"
      },
      "ssl": {
        "certfile": "/etc/murano/ssl/certs/signing_cert.pem",
        "keyfile": "/etc/murano/ssl/private/signing_key.pem",
        "generate_certs": false,
        "insecure": false,
        "cert_required": false,
        "ca_certs": "/etc/murano/ssl/certs/ca.pem"
      }
    }
  },
  "deployment": {
    "murano": {
      "crowbar-revision": 0,
      "crowbar-applied": false,
      "schema-revision": 100,
      "element_states": {
        "murano-server": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
        [ "murano-server" ]
      ],
      "element_run_list_order": {
        "murano-server": 140
      },
      "config": {
        "environment": "murano-base-config",
        "mode": "full",
        "transitions": false,
        "transition_list": [
        ]
      }
    }
  }
}
