[project]
	description = Openstack deployment for Crowbar
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Nicolas Bock (nicolasbock)
[access "refs/for/refs/*"]
	read = group user/Nicolas Bock (nicolasbock)
	push = group user/Nicolas Bock (nicolasbock)
	label-Code-Review = -2..+2 group user/Nicolas Bock (nicolasbock)
	label-Verified = -1..+1 group user/Nicolas Bock (nicolasbock)
	rebase = group user/Nicolas Bock (nicolasbock)
	removeReviewer = group user/Nicolas Bock (nicolasbock)
	submit = group user/Nicolas Bock (nicolasbock)
[access "refs/heads/*"]
	read = group user/Nicolas Bock (nicolasbock)
	create = group user/Nicolas Bock (nicolasbock)
	push = +force group user/Nicolas Bock (nicolasbock)
	pushMerge = group user/Nicolas Bock (nicolasbock)
[access "refs/tags/*"]
	push = group user/Nicolas Bock (nicolasbock)
	push = +force group user/Nicolas Bock (nicolasbock)
